{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/socket.io/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}